{
    "arrowParens": "avoid",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "cursorOffset": -1,
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "crlf",
    "experimentalOperatorPosition": "start",
    "experimentalTernaries": true,
    "htmlWhitespaceSensitivity": "css",
    "jsxSingleQuote": false,
    "objectWrap": "preserve",
    "printWidth": 120,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "useTabs": false,
    "trailingComma": "none"
}
